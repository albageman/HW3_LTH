    "def fac(x):\n",
    "    if(x==0):\n",
    "        return 0\n",
    "    return x+fac(x-1)\n",
    "\n",
    "def main():\n",
    "    for i in range(8):\n",
    "        print(fac(i*2))  \n",
    "    \n",
    "if __name__ == '__main__':\n",
    "    main()"
